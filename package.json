{
	"name": "temp-svelte-project",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run build:wasm && vite build",
		"build:full": "npm run build:wasm && vite build",
		"build:client-only": "vite build",
		"build:wasm": "cd wasm-modules/image-processor && wasm-pack build --target web --out-dir pkg && cpx \"pkg/**/*\" \"../../static/wasm-modules/image-processor/\"",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.1.11",
		"cpx": "^1.5.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.0.0",
		"vite": "^7.0.5"
	}
}
